
#
# Build configuration
#
CONFIG_BUILD_TYPE_W800S2=y
# CONFIG_BUILD_TYPE_W900 is not set
CONFIG_CHIP_NAME="W800S2"
CONFIG_BUILD_VERSION="0.3.0"
# end of Build configuration

#
# Compiler configuration
#
CONFIG_TOOLCHAIN_PATH=""
CONFIG_TOOLCHAIN_PREFIX="csky-elfabiv2-"
# CONFIG_COMPILER_OPTIMIZE_LEVEL_O0 is not set
CONFIG_COMPILER_OPTIMIZE_LEVEL_OS=y
# CONFIG_COMPILER_OPTIMIZE_LEVEL_O2 is not set
# CONFIG_COMPILER_OPTIMIZE_LEVEL_O3 is not set
CONFIG_COMPILER_DEBUG_INFORMATION=y
CONFIG_COMPILER_OPTIMIZE_CCACHE=y
# CONFIG_ATTRIBUTE_INLINE_ALWAYS is not set
# end of Compiler configuration

#
# Partition Table
#
# CONFIG_WM_PARTITION_TABLE_NORMAL_APP is not set
CONFIG_WM_PARTITION_TABLE_LARGE_APP=y
# CONFIG_WM_PARTITION_TABLE_CUSTOMIZATION is not set
CONFIG_WM_PARTITION_TABLE_OFFSET=0x1FF000
# end of Partition Table

#
# Components configuration
#
# CONFIG_BT is not set

#
# cJSON
#
# CONFIG_COMPONENT_CJSON_ENABLED is not set
# end of cJSON

#
# Coap
#
# CONFIG_COMPONENT_COAP_ENABLED is not set
# end of Coap

#
# Driver
#

#
# UART configuration
#
CONFIG_DRIVER_UART_DEFAULT_TX_PIN_1=y
# CONFIG_DRIVER_UART_DEFAULT_TX_PIN_2 is not set
CONFIG_DRIVER_UART_DEFAULT_RX_PIN_1=y
# CONFIG_DRIVER_UART_DEFAULT_RX_PIN_2 is not set
# end of UART configuration

#
# SPI configuration
#
# end of SPI configuration
# end of Driver

#
# FreeRTOS
#
CONFIG_FREERTOS_HZ=500
CONFIG_FREERTOS_IDLE_TASK_STACKSIZE=256
CONFIG_FREERTOS_MAX_TASK_NAME_LEN=16
CONFIG_FREERTOS_USE_TRACE_FACILITY=y
# CONFIG_FREERTOS_ENABLE_RUN_TIME_STATS is not set
CONFIG_FREERTOS_USE_STATS_FORMATTING_FUNCTIONS=y
CONFIG_FREERTOS_ENABLE_IDLE_HOOK=y
# CONFIG_FREERTOS_ENABLE_TICK_HOOK is not set
CONFIG_FREERTOS_QUEUE_REGISTRY_SIZE=0
CONFIG_FREERTOS_TIMER_TASK_PRIORITY=15
CONFIG_FREERTOS_TIMER_TASK_STACK_DEPTH=512
CONFIG_FREERTOS_TIMER_QUEUE_LENGTH=128
CONFIG_FREERTOS_ENABLE_STATIC_TASK_CLEAN_UP=y
CONFIG_FREERTOS_ASSERT_FAIL_ABORT=y
# CONFIG_FREERTOS_ASSERT_FAIL_PRINT_CONTINUE is not set
# CONFIG_FREERTOS_ASSERT_DISABLE is not set
CONFIG_FREERTOS_CHECK_STACKOVERFLOW=y
# end of FreeRTOS

